<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>MASES.JCOReflectorGUI</AssemblyName>
    <RootNamespace>MASES.JCOReflector.GUI</RootNamespace>
    <Title>JCOReflector - Java class generator based on JCOBridge</Title>
    <Description>JCOReflector - Java class generator based on JCOBridge</Description>
    <Copyright>Copyright (c) 2020-2025 MASES s.r.l.</Copyright>
    <Authors>MASES s.r.l.</Authors>
    <Company>MASES s.r.l.</Company>
    <Version>1.16.0.0</Version>
    <Product>MASES.JCOReflectorGUI</Product>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <TargetFrameworks>net462;net8.0-windows;net9.0-windows</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <OutputPath>..\..\..\bin\</OutputPath>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\Common\JCOReflector.snk</AssemblyOriginatorKeyFile>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Common\JCOReflector.snk" Link="JCOReflector.snk" />
  </ItemGroup>
  <ItemGroup>
	<ProjectReference Include="..\engine\JCOReflectorEngine.csproj" />
  </ItemGroup>
</Project>